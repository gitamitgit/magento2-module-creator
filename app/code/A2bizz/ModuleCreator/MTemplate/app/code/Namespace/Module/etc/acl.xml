	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="[Namespace]_[Module]::[Module]" title="[Module]" sortOrder="10">
					<resource id="[Namespace]_[Module]::[Module]_save" title="Save [Module]" sortOrder="10"/>
					<resource id="[Namespace]_[Module]::[Module]_delete" title="Delete [Module]" sortOrder="20"/>
					<resource id="[Namespace]_[Module]::[Module]_update" title="Update [Module]" sortOrder="30"/>
